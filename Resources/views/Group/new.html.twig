{% extends '::base.html.twig' %}

{% block title %}New Group{% endblock %}

{% block content %}
{{ form_start(form, {'action':  path('orkestra_group_create') , 'method': 'post' }) }}
    <div class="well">
        {{ form_widget(form) }}
        <div class="control-group">
            <div class="controls">
                <button type="submit" class="btn btn-primary">Create Group</button>
            </div>
        </div>
    </div>
{{ form_end(form) }}
{% endblock %}

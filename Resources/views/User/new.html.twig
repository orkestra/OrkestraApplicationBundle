{% extends '::base.html.twig' %}

{% block title %}New User{% endblock %}

{% block content %}
{{ form_start(form, {'action':  path('orkestra_user_create') , 'method': 'post' }) }}
<div class="well">
    {{ form_widget(form) }}
    <div class="control-group">
        <div class="controls">
            <button type="submit" class="btn btn-primary">Create User</button>
        </div>
    </div>
</div>
{{ form_end(form) }}
{% endblock %}

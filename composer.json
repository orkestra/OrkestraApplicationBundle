{
  "name": "orkestra/application-bundle",
  "description": "Provides useful base functionality for any Symfony 2 application",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=5.3.3",
    "orkestra/common": "~1.0",
    "symfony/symfony": "~2.3"
  },
  "suggest": {
    "doctrine/doctrine-fixtures-bundle": "Allows easy initial database population"
  },
  "require-dev": {
    "symfony/framework-bundle": "~2.3",
    "doctrine/orm": "~2.3",
    "doctrine/doctrine-bundle": "~1.3",
    "doctrine/doctrine-fixtures-bundle": "dev-master",
    "twig/twig": "~1.16"
  },
  "autoload": {
    "psr-0": { "Orkestra\\Bundle\\ApplicationBundle": "" }
  },
  "target-dir": "Orkestra/Bundle/ApplicationBundle",
  "extra": {
    "branch-alias": {
      "dev-develop": "1.0-dev"
    }
  }
}
